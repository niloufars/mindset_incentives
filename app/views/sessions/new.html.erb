
<p>Enter your Mechanical Turk worker ID below to begin: </p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: getid_path) do |f| %>
      <%= f.label :worker_id %>
      <%= f.text_field :worker_id, class: 'form-control' %>
      <%= f.submit "Start", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>